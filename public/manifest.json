{
    "manifest_version": 3,
    "name": "ResearchAI",
    "version": "1.0.0",
    "permissions": [
    "activeTab",
    "cookies",
    "declarativeContent",
    "scripting",
    "storage",
    "tabs",
    "clipboardRead"
  ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "index.html"
    }
  }
  